    <quoteString>N</quoteString>
    <sql>DELETE FROM av_mopublic.einzelobjekte__flaechenelement WHERE bfsnr &#x3d; &#x3f;&#x3b;&#xa;&#xa;INSERT INTO av_mopublic.einzelobjekte__flaechenelement &#x28;tid, gueltigkeit, art, geometrie, qualitaetsstandard, gwr_egid, stand_am, bfsnr&#x29;&#xa;&#xa;SELECT g.tid, g.gueltigkeit, g.typ, g.geometrie, g.qualitaetsstandard, h.gwr_egid, g.stand_am, g.bfsnr&#xa;FROM&#xa;&#x28;&#xa; SELECT b.tid, b.flaechenelement_von, e.designation_d AS gueltigkeit, c.designation_d AS typ, b.geometrie, d.designation_d AS qualitaetsstandard, NULL&#x3a;&#x3a;integer AS gwr_egid, to_date&#x28;b.stand_am, &#x27;YYYYMMDD&#x27;&#x29; as stand_am, b.gem_bfs AS bfsnr&#xa; FROM av_mopublic_meta.qualitystandard_type d, av_mopublic_meta.so_type c, &#xa; &#x28; &#xa;  SELECT b.tid, b.gem_bfs, b.flaechenelement_von, a.art, c.gueltigkeit, &#xa;                CASE&#xa;                    WHEN a.qualitaet IS NULL THEN 0&#xa;                    ELSE a.qualitaet&#xa;                END AS qualitaet, &#xa;                CASE &#xa;                    WHEN c.gueltigereintrag IS NULL THEN c.datum1&#xa;		            ELSE c.gueltigereintrag&#xa;                END AS stand_am,&#xa;                b.geometrie&#xa;   FROM av_avdpool_ch.einzelobjekte_einzelobjekt a, av_avdpool_ch.einzelobjekte_flaechenelement b, av_avdpool_ch.einzelobjekte_eonachfuehrung c&#xa;   WHERE a.gem_bfs &#x3d; &#x3f; AND b.gem_bfs &#x3d; &#x3f; AND c.gem_bfs &#x3d; &#x3f; AND a.tid&#x3a;&#x3a;text &#x3d; b.flaechenelement_von&#x3a;&#x3a;text AND c.tid&#x3a;&#x3a;text &#x3d; a.entstehung&#x3a;&#x3a;text&#xa; &#x29; b, av_mopublic_meta.validity_type e&#xa; WHERE b.art&#x3a;&#x3a;double precision &#x3d; c.code AND b.qualitaet&#x3a;&#x3a;double precision &#x3d; d.code AND b.gueltigkeit&#x3a;&#x3a;double precision &#x3d; e.code&#xa;&#x29; as g&#xa;LEFT JOIN &#xa;&#x28;&#xa; SELECT &#x2a;&#xa; FROM av_avdpool_ch.einzelobjekte_objektnummer&#xa; WHERE gem_bfs &#x3d; &#x3f;&#xa;&#x29; as h ON h.objektnummer_von &#x3d; g.flaechenelement_von&#x3b; </sql>
